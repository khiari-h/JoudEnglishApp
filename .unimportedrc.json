{
  "entry": [
    "index.js",
    "App.js",
    "src/navigation/AppNavigator.js",
    "src/navigation/index.js",
    "src/screens/index.js",
    "src/screens/**/index.js"
  ],
  "extensions": [".js", ".jsx", ".ts", ".tsx", ".json"],
  "ignorePatterns": [
    "node_modules/**",
    "__tests__/**",
    "**/*.test.js",
    "**/*.test.tsx",
    "**/*.spec.js", 
    "**/*.spec.tsx",
    "android/**",
    "ios/**",
    "web/**",
    ".expo/**",
    "dist/**",
    "build/**",
    "coverage/**",
    "metro.config.js",
    "babel.config.js",
    "jest.config.js",
    "**/*.config.js",
    "**/*.d.ts"
  ],
  "aliases": {
    "@": "./src",
    "@components": "./src/components",
    "@screens": "./src/screens", 
    "@navigation": "./src/navigation",
    "@services": "./src/services",
    "@utils": "./src/utils",
    "@hooks": "./src/hooks",
    "@assets": "./src/assets",
    "@constants": "./src/constants"
  },
  "respectGitignore": true,
  "showUnusedFiles": true,
  "showUnusedDependencies": true
}
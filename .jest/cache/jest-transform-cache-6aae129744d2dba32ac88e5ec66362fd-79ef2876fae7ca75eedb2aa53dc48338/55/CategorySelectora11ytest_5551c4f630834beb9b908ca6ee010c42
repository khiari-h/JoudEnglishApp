var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _CategorySelector = _interopRequireDefault(require("../../src/components/exercise-common/CategorySelector"));
var _jsxRuntime = require("react/jsx-runtime");
describe('CategorySelector accessibility', function () {
  it('should render categories as accessible buttons and reflect selected state', function () {
    var categories = [{
      id: 0,
      name: 'Cat 1'
    }, {
      id: 1,
      name: 'Cat 2'
    }];
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_CategorySelector.default, {
      categories: categories,
      selectedCategory: 1,
      onSelectCategory: function onSelectCategory() {}
    }));
    var items = _reactNative.screen.getAllByA11yRole('button');
    expect(items.length).toBeGreaterThanOrEqual(2);
  });
});

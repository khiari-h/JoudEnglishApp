    completedGames = _ref18[0],
    setCompletedGames = _ref18[1];
  var _ref19 = (cov_2nxtry0bfv().s[11]++, useState({
      gameIndex: 0
    })),
    _ref20 = _slicedToArray(_ref19, 2),
    lastPosition = _ref20[0],
    setLastPosition = _ref20[1];
  var _ref21 = (cov_2nxtry0bfv().s[12]++, useState(false)),
    _ref22 = _slicedToArray(_ref21, 2),
    loaded = _ref22[0],
    setLoaded = _ref22[1];
  var isInitialized = (cov_2nxtry0bfv().s[13]++, useRef(false));
  var _ref23 = (cov_2nxtry0bfv().s[14]++, useGameAnimation()),
    fadeAnim = _ref23.fadeAnim,
    bounceAnim = _ref23.bounceAnim;
  var games = (cov_2nxtry0bfv().s[15]++, (cov_2nxtry0bfv().b[2][0]++, wordGamesData == null ? void 0 : wordGamesData.games) || (cov_2nxtry0bfv().b[2][1]++, []));
  var currentGame = (cov_2nxtry0bfv().s[16]++, (cov_2nxtry0bfv().b[3][0]++, games[currentGameIndex]) || (cov_2nxtry0bfv().b[3][1]++, null));
  var totalGames = (cov_2nxtry0bfv().s[17]++, games.length);
  var STORAGE_KEY = (cov_2nxtry0bfv().s[18]++, `word_games_${level}`);
  var COMPLETED_KEY = (cov_2nxtry0bfv().s[19]++, `word_games_completed_${level}`);
  var SCORES_KEY = (cov_2nxtry0bfv().s[20]++, `word_games_scores_${level}`);
  cov_2nxtry0bfv().s[21]++;
  useEffect(function () {
    cov_2nxtry0bfv().f[1]++;
    cov_2nxtry0bfv().s[22]++;
    var loadData = function () {
      var _ref24 = _asyncToGenerator(function* () {
        cov_2nxtry0bfv().f[2]++;
        cov_2nxtry0bfv().s[23]++;
        try {
          var savedPosition = (cov_2nxtry0bfv().s[24]++, yield AsyncStorage.getItem(STORAGE_KEY));
          cov_2nxtry0bfv().s[25]++;
          if (savedPosition) {
            cov_2nxtry0bfv().b[4][0]++;
            var _ref25 = (cov_2nxtry0bfv().s[26]++, JSON.parse(savedPosition)),
              gameIndex = _ref25.gameIndex;
            cov_2nxtry0bfv().s[27]++;
            setLastPosition({
              gameIndex: gameIndex
            });
          } else {
            cov_2nxtry0bfv().b[4][1]++;
          }
          var savedCompleted = (cov_2nxtry0bfv().s[28]++, yield AsyncStorage.getItem(COMPLETED_KEY));
          cov_2nxtry0bfv().s[29]++;
          if (savedCompleted) {
            cov_2nxtry0bfv().b[5][0]++;
            cov_2nxtry0bfv().s[30]++;
            setCompletedGames(JSON.parse(savedCompleted));
          } else {
            cov_2nxtry0bfv().b[5][1]++;
          }
          cov_2nxtry0bfv().s[31]++;
          if (games.length > 0) {
            cov_2nxtry0bfv().b[6][0]++;
            cov_2nxtry0bfv().s[32]++;
            setGameResults(Array(games.length).fill({
              score: 0,
              maxScore: 0,
              completed: false
            }));
          } else {
            cov_2nxtry0bfv().b[6][1]++;
          }
        } catch (error) {} finally {
          cov_2nxtry0bfv().s[33]++;
          setLoaded(true);
        }
      });
      return function loadData() {
        return _ref24.apply(this, arguments);
      };
    }();
    cov_2nxtry0bfv().s[34]++;
    loadData();
  }, [level, games.length]);
  var saveData = (cov_2nxtry0bfv().s[35]++, useCallback(_asyncToGenerator(function* () {
    cov_2nxtry0bfv().f[3]++;
    cov_2nxtry0bfv().s[36]++;
    try {
      var dataToSave = (cov_2nxtry0bfv().s[37]++, {
        gameIndex: currentGameIndex,
        timestamp: Date.now()
      });
      cov_2nxtry0bfv().s[38]++;
      yield AsyncStorage.setItem(STORAGE_KEY, JSON.stringify(dataToSave));
      cov_2nxtry0bfv().s[39]++;
      yield AsyncStorage.setItem(COMPLETED_KEY, JSON.stringify(completedGames));
    } catch (error) {}
  }), [currentGameIndex, completedGames, STORAGE_KEY, COMPLETED_KEY]));
  cov_2nxtry0bfv().s[40]++;
  useEffect(function () {
    cov_2nxtry0bfv().f[4]++;
    cov_2nxtry0bfv().s[41]++;
    if (loaded) {
      cov_2nxtry0bfv().b[7][0]++;
      cov_2nxtry0bfv().s[42]++;
      saveData();
    } else {
      cov_2nxtry0bfv().b[7][1]++;
    }
  }, [saveData, loaded]);
  cov_2nxtry0bfv().s[43]++;
  useEffect(function () {
    cov_2nxtry0bfv().f[5]++;
    cov_2nxtry0bfv().s[44]++;
    if ((cov_2nxtry0bfv().b[9][0]++, loaded) && (cov_2nxtry0bfv().b[9][1]++, games.length > 0) && (cov_2nxtry0bfv().b[9][2]++, !isInitialized.current)) {
      cov_2nxtry0bfv().b[8][0]++;
      cov_2nxtry0bfv().s[45]++;
      if ((cov_2nxtry0bfv().b[11][0]++, lastPosition) && (cov_2nxtry0bfv().b[11][1]++, lastPosition.gameIndex < games.length)) {
        cov_2nxtry0bfv().b[10][0]++;
        cov_2nxtry0bfv().s[46]++;
        setCurrentGameIndex(lastPosition.gameIndex);
      } else {
        cov_2nxtry0bfv().b[10][1]++;
      }
      var newCompletedGames = (cov_2nxtry0bfv().s[47]++, Object.assign({}, completedGames));
      cov_2nxtry0bfv().s[48]++;
      games.forEach(function (_, index) {
        cov_2nxtry0bfv().f[6]++;
        cov_2nxtry0bfv().s[49]++;
        if (!newCompletedGames[index]) {
          cov_2nxtry0bfv().b[12][0]++;
          cov_2nxtry0bfv().s[50]++;
          newCompletedGames[index] = {
            completed: false
          };
        } else {
          cov_2nxtry0bfv().b[12][1]++;
        }
      });
      cov_2nxtry0bfv().s[51]++;
      setCompletedGames(newCompletedGames);
      cov_2nxtry0bfv().s[52]++;
      isInitialized.current = true;
    } else {
      cov_2nxtry0bfv().b[8][1]++;
    }
  }, [loaded, games, lastPosition, completedGames]);
  cov_2nxtry0bfv().s[53]++;
  var shuffleGameOptions = function shuffleGameOptions(game) {
    cov_2nxtry0bfv().f[7]++;
    var optionsToShuffle = (cov_2nxtry0bfv().s[54]++, []);
    cov_2nxtry0bfv().s[55]++;
    if (game.type === "matching") {
      cov_2nxtry0bfv().b[13][0]++;
      var allItems = (cov_2nxtry0bfv().s[56]++, game.pairs.flatMap(function (pair) {
        cov_2nxtry0bfv().f[8]++;
        cov_2nxtry0bfv().s[57]++;
        return [pair.word, pair.match];
      }));
      cov_2nxtry0bfv().s[58]++;
      optionsToShuffle = shuffleArray(allItems);
    } else {
      cov_2nxtry0bfv().b[13][1]++;
      cov_2nxtry0bfv().s[59]++;
      if (game.type === "categorization") {
        cov_2nxtry0bfv().b[14][0]++;
        cov_2nxtry0bfv().s[60]++;
        optionsToShuffle = shuffleArray(_toConsumableArray(game.words));
      } else {
        cov_2nxtry0bfv().b[14][1]++;
      }
    }
    cov_2nxtry0bfv().s[61]++;
    setShuffledOptions(optionsToShuffle);
  };
  cov_2nxtry0bfv().s[62]++;
  useEffect(function () {
    cov_2nxtry0bfv().f[9]++;
    cov_2nxtry0bfv().s[63]++;
    if (currentGame) {
      cov_2nxtry0bfv().b[15][0]++;
      cov_2nxtry0bfv().s[64]++;
      setSelectedItems([]);
      cov_2nxtry0bfv().s[65]++;
      setMatchedItems([]);
      cov_2nxtry0bfv().s[66]++;
      setShowFeedback(false);
      cov_2nxtry0bfv().s[67]++;
      setIsCorrect(false);
      cov_2nxtry0bfv().s[68]++;
      shuffleGameOptions(currentGame);
    } else {
      cov_2nxtry0bfv().b[15][1]++;
    }
  }, [currentGameIndex, currentGame]);
  var handleNext = (cov_2nxtry0bfv().s[69]++, useCallback(function () {
    cov_2nxtry0bfv().f[10]++;
    cov_2nxtry0bfv().s[70]++;
    if (currentGameIndex < games.length - 1) {
      cov_2nxtry0bfv().b[16][0]++;
      cov_2nxtry0bfv().s[71]++;
      setCurrentGameIndex(function (prev) {
        cov_2nxtry0bfv().f[11]++;
        cov_2nxtry0bfv().s[72]++;
        return prev + 1;
      });
      cov_2nxtry0bfv().s[73]++;
      setShowFeedback(false);
      cov_2nxtry0bfv().s[74]++;
      setIsCorrect(false);
      cov_2nxtry0bfv().s[75]++;
      return {
        completed: false
      };
    } else {
      cov_2nxtry0bfv().b[16][1]++;
      cov_2nxtry0bfv().s[76]++;
      setShowResults(true);
      cov_2nxtry0bfv().s[77]++;
      return {
        completed: true
      };
    }
  }, [currentGameIndex, games.length]));
  var handlePrevious = (cov_2nxtry0bfv().s[78]++, useCallback(function () {
    cov_2nxtry0bfv().f[12]++;
    cov_2nxtry0bfv().s[79]++;
    if (currentGameIndex > 0) {
      cov_2nxtry0bfv().b[17][0]++;
      cov_2nxtry0bfv().s[80]++;
      setCurrentGameIndex(function (prev) {
        cov_2nxtry0bfv().f[13]++;
        cov_2nxtry0bfv().s[81]++;
        return prev - 1;
      });
      cov_2nxtry0bfv().s[82]++;
      setShowFeedback(false);
      cov_2nxtry0bfv().s[83]++;
      setIsCorrect(false);
      cov_2nxtry0bfv().s[84]++;
      return true;
    } else {
      cov_2nxtry0bfv().b[17][1]++;
    }
    cov_2nxtry0bfv().s[85]++;
    return false;
  }, [currentGameIndex]));
  var resetGames = (cov_2nxtry0bfv().s[86]++, useCallback(_asyncToGenerator(function* () {
    cov_2nxtry0bfv().f[14]++;
    cov_2nxtry0bfv().s[87]++;
    try {
      cov_2nxtry0bfv().s[88]++;
      yield AsyncStorage.multiRemove([STORAGE_KEY, COMPLETED_KEY, SCORES_KEY]);
      cov_2nxtry0bfv().s[89]++;
      setCurrentGameIndex(0);
      cov_2nxtry0bfv().s[90]++;
      setSelectedItems([]);
      cov_2nxtry0bfv().s[91]++;
      setMatchedItems([]);
      cov_2nxtry0bfv().s[92]++;
      setShowFeedback(false);
      cov_2nxtry0bfv().s[93]++;
      setIsCorrect(false);
      cov_2nxtry0bfv().s[94]++;
      setShowResults(false);
      cov_2nxtry0bfv().s[95]++;
      setScore(0);
      cov_2nxtry0bfv().s[96]++;
      setCompletedGames({});
      cov_2nxtry0bfv().s[97]++;
      setLastPosition({
        gameIndex: 0
      });
      cov_2nxtry0bfv().s[98]++;
      if (games.length > 0) {
        cov_2nxtry0bfv().b[18][0]++;
        cov_2nxtry0bfv().s[99]++;
        setGameResults(Array(games.length).fill({
          score: 0,
          maxScore: 0,
          completed: false
        }));
        cov_2nxtry0bfv().s[100]++;
        shuffleGameOptions(games[0]);
      } else {
        cov_2nxtry0bfv().b[18][1]++;
      }
    } catch (error) {}
  }), [STORAGE_KEY, COMPLETED_KEY, SCORES_KEY, games]));
  var getStats = (cov_2nxtry0bfv().s[101]++, useCallback(function () {
    cov_2nxtry0bfv().f[15]++;
    var completedGamesCount = (cov_2nxtry0bfv().s[102]++, Object.values(completedGames).filter(function (game) {
      cov_2nxtry0bfv().f[16]++;
      cov_2nxtry0bfv().s[103]++;
      return game.completed;
    }).length);
    var totalMaxScore = (cov_2nxtry0bfv().s[104]++, gameResults.reduce(function (sum, result) {
      cov_2nxtry0bfv().f[17]++;
      cov_2nxtry0bfv().s[105]++;
      return sum + result.maxScore;
    }, 0));
    var totalProgress = (cov_2nxtry0bfv().s[106]++, totalGames > 0 ? (cov_2nxtry0bfv().b[19][0]++, Math.round(completedGamesCount / totalGames * 100)) : (cov_2nxtry0bfv().b[19][1]++, 0));
    var currentProgress = (cov_2nxtry0bfv().s[107]++, totalGames > 0 ? (cov_2nxtry0bfv().b[20][0]++, (currentGameIndex + 1) / totalGames * 100) : (cov_2nxtry0bfv().b[20][1]++, 0));
    cov_2nxtry0bfv().s[108]++;
    return {
      totalGames: totalGames,
      completedGamesCount: completedGamesCount,
      totalProgress: totalProgress,
      currentProgress: currentProgress,
      score: score,
      totalMaxScore: totalMaxScore,
      percentage: totalMaxScore > 0 ? (cov_2nxtry0bfv().b[21][0]++, Math.round(score / totalMaxScore * 100)) : (cov_2nxtry0bfv().b[21][1]++, 0)
    };
  }, [totalGames, completedGames, gameResults, currentGameIndex, score]));
  var getDisplayData = (cov_2nxtry0bfv().s[109]++, useCallback(function () {
    cov_2nxtry0bfv().f[18]++;
    var gameCounter = (cov_2nxtry0bfv().s[110]++, `${currentGameIndex + 1} / ${totalGames}`);
    var gameTitle = (cov_2nxtry0bfv().s[111]++, (cov_2nxtry0bfv().b[22][0]++, currentGame == null ? void 0 : currentGame.title) || (cov_2nxtry0bfv().b[22][1]++, `Game ${currentGameIndex + 1}`));
    cov_2nxtry0bfv().s[112]++;
    return {
      gameCounter: gameCounter,
      gameTitle: gameTitle,
      currentGame: currentGame,
      currentGameIndex: currentGameIndex + 1
    };
  }, [currentGameIndex, totalGames, currentGame]));
  var canGoToPrevious = (cov_2nxtry0bfv().s[113]++, currentGameIndex > 0);
  var isLastGame = (cov_2nxtry0bfv().s[114]++, currentGameIndex === totalGames - 1);
  cov_2nxtry0bfv().s[115]++;
  return {
    currentGameIndex: currentGameIndex,
    selectedItems: selectedItems,
    matchedItems: matchedItems,
    showFeedback: showFeedback,
    isCorrect: isCorrect,
    showResults: showResults,
    score: score,
    gameResults: gameResults,
    shuffledOptions: shuffledOptions,
    completedGames: completedGames,
    loaded: loaded,
    games: games,
    currentGame: currentGame,
    totalGames: totalGames,
    fadeAnim: fadeAnim,
    bounceAnim: bounceAnim,
    handleNext: handleNext,
    handlePrevious: handlePrevious,
    resetGames: resetGames,
    setCurrentGameIndex: setCurrentGameIndex,
    canGoToPrevious: canGoToPrevious,
    isLastGame: isLastGame,
    stats: getStats(),
    display: getDisplayData()
  };
};
export default useWordGames;

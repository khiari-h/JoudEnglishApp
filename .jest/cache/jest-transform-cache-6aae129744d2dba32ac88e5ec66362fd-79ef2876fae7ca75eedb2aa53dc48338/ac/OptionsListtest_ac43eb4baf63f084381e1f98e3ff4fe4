        getByTestId = _render5.getByTestId;
      var optionStyle = _reactNative2.StyleSheet.flatten(getByTestId('option-item-1').props.style);
      expect(optionStyle).toEqual(expect.objectContaining({
        flexDirection: 'row'
      }));
    });

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _HeroCard = _interopRequireDefault(require("../../../src/components/ui/HeroCard"));
var _reactNative2 = require("react-native");
var _jsxRuntime = require("react/jsx-runtime");
describe('HeroCard', function () {
  it('renders correctly with content', function () {
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
      content: "Hello World"
    }));
    expect(_reactNative.screen.getByText('Hello World')).toBeOnTheScreen();
  });
  it('renders correctly with children', function () {
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
      children: (0, _jsxRuntime.jsx)(_reactNative2.Text, {
        children: "Child Content"
      })
    }));
    expect(_reactNative.screen.getByText('Child Content')).toBeOnTheScreen();
  });
  it('applies custom font size', function () {
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
      content: "Large Text",
      fontSize: 40
    }));
    var textElement = _reactNative.screen.getByText('Large Text');
    expect(textElement).toBeOnTheScreen();
  });
  it('applies custom text alignment', function () {
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
      content: "Right Aligned",
      textAlign: "right"
    }));
    var textElement = _reactNative.screen.getByText('Right Aligned');
    expect(textElement).toBeOnTheScreen();
  });
  it('does not show underline when showUnderline is false', function () {
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
      content: "No Underline",
      showUnderline: false
    }));
    expect(_reactNative.screen.getByText('No Underline')).toBeOnTheScreen();
  });
  it('applies custom containerStyle', function () {
    var customStyle = {
      padding: 20
    };
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
      content: "Styled Card",
      containerStyle: customStyle
    }));
    expect(_reactNative.screen.getByText('Styled Card')).toBeOnTheScreen();
  });
  it('does not render content if it is empty string or not a string', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
        content: ""
      })),
      queryByText = _render.queryByText;
    expect(queryByText('')).toBeNull();
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
        content: null
      })),
      queryByTextNull = _render2.queryByText;
    expect(queryByTextNull(null)).toBeNull();
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_HeroCard.default, {
        content: undefined
      })),
      queryByTextUndefined = _render3.queryByText;
    expect(queryByTextUndefined(undefined)).toBeNull();
  });
});

    };
  });
};
var convertFastVocabToExercises = function convertFastVocabToExercises(fastVocab) {
  if (!fastVocab || !fastVocab.words) {
    return {
      exercises: []
    };
  }
  return {
    exercises: [{
      title: fastVocab.title || "Vocabulaire Fast",
      words: fastVocab.words
    }]
  };
};

  it('should render categories as accessible buttons and reflect selected state', function () {
    var categories = [{
      id: 0,
      name: 'Cat 1'
    }, {
      id: 1,
      name: 'Cat 2'
    }];
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_CategorySelector.default, {

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _ProgressBar = _interopRequireDefault(require("../../src/components/ui/ProgressBar"));
var _jsxRuntime = require("react/jsx-runtime");
describe('ProgressBar accessibility', function () {
  it('should expose progressbar role and value', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_ProgressBar.default, {
        progress: 42,
        label: "Avancement",
        showPercentage: true
      })),
      getByA11yRole = _render.getByA11yRole;
    var bar = getByA11yRole('progressbar');
    expect(bar.props.accessibilityValue).toMatchObject({
      min: 0,
      max: 100,
      now: 42
    });
  });
});

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _Modal = _interopRequireDefault(require("../../src/components/ui/Modal"));
var _jsxRuntime = require("react/jsx-runtime");
describe('Modal accessibility', function () {
  it('should expose an accessible close button with label', function () {
    var onClose = jest.fn();
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Modal.default, {
        visible: true,
        onClose: onClose,
        title: "Titre"
      })),
      getByA11yLabel = _render.getByA11yLabel;
    _reactNative.fireEvent.press(getByA11yLabel('Close modal'));
    expect(onClose).toHaveBeenCalled();
  });
});

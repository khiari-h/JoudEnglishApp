              }), (cov_212zwypz3n().b[11][0]++, isCorrect) && (cov_212zwypz3n().b[11][1]++, styles.correctIndicator), (cov_212zwypz3n().b[12][0]++, isIncorrect) && (cov_212zwypz3n().b[12][1]++, styles.incorrectIndicator)],
              children: _jsxs(Text, {
                style: [styles.choiceIndicatorText, (cov_212zwypz3n().b[14][0]++, isSelected) && (cov_212zwypz3n().b[14][1]++, !showFeedback) || (cov_212zwypz3n().b[14][2]++, isCorrect) || (cov_212zwypz3n().b[14][3]++, isIncorrect) ? (cov_212zwypz3n().b[13][0]++, {
                  color: 'white'
                }) : (cov_212zwypz3n().b[13][1]++, {
                  color: '#64748b'
                })],
                children: [String.fromCharCode(65 + index), " "]
              })
            }), _jsx(Text, {
              style: [styles.choiceText, (cov_212zwypz3n().b[15][0]++, isSelected) && (cov_212zwypz3n().b[15][1]++, !showFeedback) && (cov_212zwypz3n().b[15][2]++, {
                color: levelColor,
                fontWeight: '600'
              }), (cov_212zwypz3n().b[16][0]++, isCorrect) && (cov_212zwypz3n().b[16][1]++, styles.correctChoiceText), (cov_212zwypz3n().b[17][0]++, isIncorrect) && (cov_212zwypz3n().b[17][1]++, styles.incorrectChoiceText)],
              children: choice
            })]
          })
        }, index);
      })
    }), (cov_212zwypz3n().b[18][0]++, showFeedback) && (cov_212zwypz3n().b[18][1]++, exercise.explanation) && (cov_212zwypz3n().b[18][2]++, _jsx(ContentSection, {
      title: "Explication",
      content: exercise.explanation,
      levelColor: selectedChoiceIndex === correctChoiceIndex ? (cov_212zwypz3n().b[19][0]++, "#10b981") : (cov_212zwypz3n().b[19][1]++, "#ef4444"),
      backgroundColor: selectedChoiceIndex === correctChoiceIndex ? (cov_212zwypz3n().b[20][0]++, "#f0fdf4") : (cov_212zwypz3n().b[20][1]++, "#fef2f2"),
      style: styles.feedbackSection
    }))]
  });
};
cov_212zwypz3n().s[15]++;
MultipleChoiceMode.propTypes = {
  exercise: PropTypes.shape({
    text: PropTypes.string,
    choices: PropTypes.arrayOf(PropTypes.string).isRequired,
    correctChoiceIndex: PropTypes.number.isRequired,
    explanation: PropTypes.string
  }).isRequired,
  selectedChoiceIndex: PropTypes.number,
  onSelectChoice: PropTypes.func.isRequired,
  showFeedback: PropTypes.bool,
  levelColor: PropTypes.string
};
export default MultipleChoiceMode;

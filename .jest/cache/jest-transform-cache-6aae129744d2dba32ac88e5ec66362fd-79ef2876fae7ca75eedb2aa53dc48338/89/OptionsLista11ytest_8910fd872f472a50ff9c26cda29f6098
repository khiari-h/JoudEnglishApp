var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _OptionsList = _interopRequireDefault(require("../../src/components/exercise-common/OptionsList"));
var _jsxRuntime = require("react/jsx-runtime");
describe('OptionsList accessibility', function () {
  it('should render options as accessible buttons with text labels', function () {
    var options = [{
      id: 'a',
      text: 'Option A'
    }, {
      id: 'b',
      text: 'Option B'
    }];
    (0, _reactNative.render)((0, _jsxRuntime.jsx)(_OptionsList.default, {
      options: options,
      selectedOptionId: 'a',
      onSelectOption: function onSelectOption() {}
    }));
    var buttons = _reactNative.screen.getAllByA11yRole('button');
    expect(buttons.length).toBeGreaterThanOrEqual(2);
  });
});

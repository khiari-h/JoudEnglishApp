describe('OptionsList accessibility', function () {
  it('should render options as accessible buttons with text labels', function () {
    var options = [{
      id: 'a',
      text: 'Option A'
    }, {
      id: 'b',
      text: 'Option B'
    }];
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_OptionsList.default, {
        options: options,
        selectedOptionId: 'a',
        onSelectOption: function onSelectOption() {}
      })),
      getAllByRole = _render.getAllByRole;
    var buttons = getAllByRole('button');
    expect(buttons.length).toBeGreaterThanOrEqual(2);
  });
